.course-reservation-section{width:100%;display:flex;justify-content:space-between;gap:32px}
.course-section{width:100%}
.course-image-wrapper{width:100%}
.wrapper-back{position:absolute;bottom:0;left:0;width:100%;height:100%;background:linear-gradient(to top,rgba(0,0,0,.7) 0%,rgba(0,0,0,.3) 40%,transparent 100%);z-index:0}
.course-image-img{object-fit:cover;width:100%;height:100%;max-width:1200px;max-height:50vh}
.course-image-text{display:flex;width:100%;flex-direction:column;gap:8px}
.course-image-icon{display:flex;align-items:center;gap:6px}
.course-image-icon img{width:34px}
.course-content{width:100%;display:flex;flex-direction:column;gap:16px;margin-top:16px;margin-bottom:10vw}
.course-content > div{display:flex}
.course-content-flex{display:flex;align-items:flex-start;gap:28px;justify-content:space-between;flex-direction:row}
.course-content-title{display:flex;align-items:center;gap:6px}
.course-content-title img{width:26px}
.course-content-list{display:flex;align-items:flex-start;gap:8px;flex-direction:column}
.reservation-section{width:470px;min-width:470px}
.reservation-box{display:flex;width:100%;background-color:#fff;box-shadow:0 4px 6px rgba(0,0,0,.1);flex-direction:column;gap:16px}
.reservation-price-flex{height:60px;display:flex;width:100%;align-items:center;justify-content:space-between}
.reservation-return{display:flex;align-items:center;gap:4px}
.reservation-return img{width:20px}
.reservation-users{display:flex;width:100%;gap:12px;flex-wrap:wrap;align-items:flex-start}
.reservation-user-box{display:flex;flex-direction:column;gap:6px;align-items:center;justify-content:center;width:110px;min-width:110px;max-width:110px;opacity:.6}
.reservation-user-title{display:flex;align-items:center;gap:3px;justify-content:center;text-align:center}
.reservation-user-title img{width:16px}
.reservation-user-bullet{width:8px;height:8px;border-radius:12px;background-color:var(--primary-bs)}
.reservation-user-add{background:var(--text-color-medium);width:70px;height:70px;border-radius:70px;display:flex;justify-content:center;align-items:center}
.reservation-user-add img{width:34px}
.tooltip-container{position:absolute;left:50%;bottom:100%;transform:translateX(-50%);background:#ddd;color:#000;padding:12px;border-radius:6px;font-size:13px;white-space:normal;max-width:350px;width:350px;box-shadow:0 4px 6px rgba(0,0,0,.2);transition:opacity .2s;text-align:left;overflow-wrap:break-word}
.tooltip-title{font-weight:bold;margin-bottom:8px;font-size:14px;text-align:center}
ul{padding-left:20px;margin:0;list-style-type:disc;font-size:12px}
.tooltip-container::after{content:"";position:absolute;top:100%;left:50%;transform:translateX(-50%);border-width:6px;border-style:solid;border-color:rgba(0,0,0,.85) transparent transparent transparent}
@media (max-width:600px){.tooltip-container{max-width:90%}}
.custom-checkbox{width:100%;border-radius:3px;border:1px solid rgba(34,34,34,1);position:relative;padding:12px;display:flex;flex-direction:column}
.checkbox-content{display:flex;align-items:center;justify-content:space-between;width:100%}
.level-text{font-size:12px;line-height:16px}
.checkbox-container{position:relative;width:100%}
.info-icon{width:24px;height:24px;cursor:pointer;position:absolute;right:10px;top:50%;transform:translateY(-50%)}
.levels-section{display:flex;width:100%;gap:12px;flex-wrap:wrap;justify-content:space-between;flex-direction:column}
.level-wrapper{background-color:var(--level-background);height:70px;width:calc(50% - 6px);padding:8px;display:flex;flex-direction:column;align-items:flex-start;justify-content:space-between;position:relative}
.level-check{position:absolute;top:8px;right:8px;width:18px;height:18px;border-radius:18px;background-color:var(--primary-bs);display:flex;justify-content:center;align-items:center}
.level-check img{width:10px}
.calendar{width:100%;box-sizing:border-box;padding:16px}
.header-calendar{display:flex;align-items:center;justify-content:space-between;margin-bottom:20px}
.header-calendar-arrows{display:flex;align-items:center;gap:16px}
.header-calendar-arrows img{cursor:pointer;width:26px}
.weekdays,.days{display:flex;flex-wrap:wrap;justify-content:space-between}
.weekdays span{flex:1 0 calc(14.28% - 6px);height:32px;line-height:32px;text-align:center;margin:3px;box-sizing:border-box}
.days .day-span-a{flex:1 0 calc(14.28% - 6px);height:32px;line-height:32px;text-align:center;margin:3px;font-weight:500;box-sizing:border-box}
.days .day-span-b{display:flex;justify-content:center;width:32px;height:32px;align-items:center}
.light-opacity{opacity:.6}
.very-light-opacity{opacity:.4}
.active-collective{opacity:1;background-color:rgba(217,177,32,.6);border-radius:50%;cursor:pointer;transition:all .2s }
.active-collective-selected{opacity:1;background-color:rgba(217,177,32,1);border-radius:50%;cursor:pointer;transition:all .2s }
.active-private{opacity:1;background-color:rgba(194,217,69,.6);border-radius:50%;cursor:pointer;transition:all .2s }
.active-private-selected{opacity:1;background-color:rgba(194,217,69,1);border-radius:50%;cursor:pointer;transition:all .2s }
.calendar-reservation-form{display:flex;gap:4px;flex-direction:column}
.calendar-reservation-form-input{width:150px;display:flex}
.opacity-full{opacity:1 !important}
.tooltip-container-level{padding:10px;border-radius:4px;position:absolute;z-index:1000;bottom:70px;left:0;width:200px;max-width:200px}
.tooltip-container-level div{padding:2px 0}
.show-mobile{display:none !important}
.show-desktop{display:block !important}
@media (max-width:800px){.course-reservation-section{flex-direction:column}.reservation-section{position:fixed;bottom:0;left:0;width:100vw;min-width:0;background-color:#fff;box-shadow:0 8px 16px 0 var(--box-shadow-color);border-top-left-radius:8px;border-top-right-radius:8px}.reservation-section button{width:calc(50% - 8px)}.reservation-box{flex-wrap:wrap;flex-direction:row}}
.descripcion{color:#61656d}
.descripcion2{color:#8b9099;font-size:12px;font-weight:600;line-height:16px}
.price{font-size:18px;font-family:Dinamit;font-weight:500;line-height:23px}
.campo{font-weight:600}
.titlexxxx{color:#fff;font-size:16px;font-family:Dinamit;font-weight:500;line-height:20px;background-color:rgba(47,56,68,1);height:60px;padding:20px;border-radius:8px 8px 0 0;display:flex;gap:20px}
.reservation-view{@media (max-width:800px){overflow-y:auto;height:100vh;display:flex;flex-direction:column;width:95%;margin:auto}.reservation-view .titlexxxx{height:64px;background:#fff}.reservation-view .reservation-view2{margin-top:auto}}
.last-plaza{position:absolute;top:10px;right:10px;color:#fff;background-color:#e91e63;border-radius:3px;font-weight:600;padding:4px 10px}
.campo2{display:flex;justify-content:space-between;width:100%;gap:20px}
.placeLevel{font-size:12px;line-height:16px;word-wrap:break-word;cursor:pointer}
::ng-deep{.mdc-checkbox .mdc-checkbox__native-control:enabled ~ .mdc-checkbox__background .mdc-checkbox__checkmark{display:none !important}.mdc-checkbox .mdc-checkbox__native-control:enabled:not(:checked):not(:indeterminate):not([data-indeterminate="true"]) ~ .mdc-checkbox__background{background-color:#fff !important}}
.extra_product{font-weight:600;word-wrap:break-word}
.extra_name{color:#8b9099;word-wrap:break-word}
.dates-detail-container{width:100%;padding:8px 0;margin-bottom:16px}
.dates-section-title{font-size:16px;font-weight:600;color:var(--color-dark1);margin-bottom:12px;padding-left:32px}
.intervals-list,.weeks-list{width:100%;display:flex;flex-direction:column;gap:16px;padding-left:32px}
.interval-card,.week-card{width:100%;border:1px solid #e0e0e0;border-radius:8px;overflow:hidden;background-color:#fff;box-shadow:0 2px 4px rgba(0,0,0,.05);transition:box-shadow .3s}
.interval-card:hover,.week-card:hover{box-shadow:0 4px 8px rgba(0,0,0,.1)}
.interval-header,.week-header{padding:16px;background-color:#f8f9fa;display:flex;justify-content:space-between;align-items:center;cursor:pointer;border-bottom:1px solid #e0e0e0;transition:background-color .2s}
.interval-header:hover,.week-header:hover{background-color:#f1f3f5}
.interval-title-section,.week-title-section{display:flex;align-items:center;gap:12px}
.interval-name,.week-name{font-size:16px;font-weight:600;color:var(--color-dark1);margin:0}
.interval-summary,.week-summary{display:flex;flex-wrap:wrap;align-items:center;gap:8px;color:var(--color-dark2)}
.interval-dates,.week-dates{font-weight:500}
.interval-weekdays,.week-weekdays{color:var(--color-dark3);font-style:italic}
.interval-time,.week-time{background-color:#e9ecef;padding:4px 8px;border-radius:4px;font-size:12px}
.toggle-icon{display:flex;align-items:center;justify-content:center}
.toggle-icon img{width:20px;height:20px;transition:transform .3s}
.interval-content,.week-content{padding:16px;background-color:#fff;animation:fadeIn .3s}
@keyframes fadeIn{from{opacity:0;transform:translateY(-10px)}to{opacity:1;transform:translateY(0)}}
.dates-grid{display:grid;grid-template-columns:repeat(auto-fit,minmax(85px,1fr));gap:6px;max-width:100%;max-height:300px;overflow-y:auto}
@media (min-width:600px){.dates-grid{grid-template-columns:repeat(auto-fit,minmax(90px,1fr));gap:6px}}
@media (min-width:768px){.dates-grid{grid-template-columns:repeat(auto-fit,minmax(90px,1fr));gap:8px}}
@media (min-width:1024px){.dates-grid{grid-template-columns:repeat(auto-fit,minmax(90px,1fr));gap:8px}}
@media (min-width:1200px){.dates-grid{grid-template-columns:repeat(auto-fit,minmax(90px,1fr));gap:10px}}
.date-item{background-color:#fff;border-radius:4px;padding:6px 4px;display:flex;flex-direction:column;gap:2px;border:1px solid #e0e0e0;box-shadow:0 1px 3px rgba(0,0,0,.05);transition:transform .2s,box-shadow .2s;min-width:80px;text-align:center}
.date-item:hover{transform:translateY(-1px);box-shadow:0 3px 6px rgba(0,0,0,.1);border-color:#ccc}
.date-item .date-day{display:flex;flex-direction:column;align-items:center;margin-bottom:2px}
.date-item .date-day .day-number{font-size:18px;font-weight:700;line-height:1;color:var(--color-dark1)}
.date-item .date-day .month-year{font-size:9px;color:var(--color-dark3);text-transform:uppercase;letter-spacing:.3px}
.date-item .date-weekday{font-size:10px;font-weight:600;color:var(--color-primary,#e91e63);text-transform:uppercase;letter-spacing:.3px}
.date-item .date-time{background-color:rgba(233,30,99,.1);color:var(--color-primary,#e91e63);padding:3px 4px;border-radius:3px;font-size:9px;font-weight:600}
.simple-dates-list{padding-left:32px;width:100%}
.private-course-dates{padding-left:32px;margin-top:12px}
.week-info-container{background-color:#f8f9fa;border-radius:8px;padding:16px;border:1px solid #e0e0e0}
.weekdays-info{font-size:15px;font-weight:600;margin-bottom:12px;color:var(--color-dark1)}
.duration-info,.hours-info{display:flex;gap:8px;margin-top:8px;color:var(--color-dark2)}
.duration-label,.hours-label{font-weight:500}
.duration-value,.hours-value{font-weight:600}
.no-dates{background-color:#f8f9fa;padding:16px;text-align:center;border-radius:8px;color:#6c757d;font-style:italic;border:1px solid #e0e0e0}
@media (max-width:768px){.dates-grid{grid-template-columns:repeat(auto-fit,minmax(120px,1fr));gap:8px}.interval-header,.week-header{flex-direction:column;align-items:flex-start;gap:8px}.interval-summary,.week-summary{width:100%}.toggle-icon{position:absolute;top:16px;right:16px}.interval-card,.week-card{position:relative}.intervals-list,.weeks-list,.simple-dates-list,.private-course-dates{padding-left:0}.dates-section-title{padding-left:0}}
.date-selection-container{padding:20px;background-color:#f8f9fa;border-radius:8px;border:1px solid #e0e0e0;margin-bottom:20px}
.date-selection-title{font-size:16px;font-weight:600;margin-bottom:16px;color:var(--color-dark1)}
.date-selection-error{background-color:#ffebee;border-radius:4px;padding:8px 12px;margin-bottom:16px;display:flex;align-items:center;gap:8px;color:#d32f2f;font-size:14px}
.date-selection-error mat-icon{font-size:20px;height:20px;width:20px}
.booking-restrictions{background-color:#e3f2fd;border-radius:4px;padding:12px 16px;margin-bottom:20px}
.restriction-title{font-weight:600;margin-bottom:8px;color:#0d47a1}
.restriction-list{list-style-type:none;padding:0;margin:0}
.restriction-list li{display:flex;align-items:center;gap:8px;margin-bottom:6px;font-size:14px}
.info-tooltip{color:#64b5f6;cursor:pointer;font-size:18px;height:18px;width:18px}
.interval-selection-container{margin-top:16px}
.intervals-list{display:flex;flex-direction:column;gap:12px}
.interval-card{border:1px solid #e0e0e0;border-radius:8px;overflow:hidden;transition:all .3s}
.interval-card:hover:not(.interval-disabled){box-shadow:0 2px 8px rgba(0,0,0,.1);border-color:#bbdefb}
.interval-selected{border-color:#2196f3;box-shadow:0 0 0 2px rgba(33,150,243,.2)}
.interval-disabled{opacity:.5;cursor:not-allowed}
.interval-header{padding:12px 16px;background-color:#f8f9fa;display:flex;justify-content:space-between;align-items:center;cursor:pointer;transition:background-color .2s}
.interval-header:hover:not(.interval-disabled){background-color:#e3f2fd}
.interval-selected .interval-header{background-color:#e3f2fd}
.interval-title-section{display:flex;align-items:center;gap:8px}
.interval-title-section mat-icon{color:#2196f3}
.interval-name{margin:0;font-size:15px;font-weight:600}
.interval-summary{display:flex;flex-wrap:wrap;gap:8px;align-items:center;font-size:14px;color:var(--color-dark2)}
.interval-dates{font-weight:500}
.interval-count{color:var(--color-dark3);font-size:13px}
.interval-dates-container{padding:12px 16px;background-color:#fff;animation:fadeIn .3s}
.date-checkboxes{display:flex;flex-wrap:wrap;gap:12px}
.date-checkbox-wrapper{position:relative;flex:1 0 calc(33.33% - 12px);min-width:150px;max-width:200px;border:1px solid #e0e0e0;border-radius:6px;overflow:hidden;transition:all .2s}
.date-checkbox-wrapper:hover:not(.date-disabled){border-color:#bbdefb;box-shadow:0 2px 6px rgba(0,0,0,.1)}
.date-selected{border-color:#2196f3;box-shadow:0 0 0 2px rgba(33,150,243,.2)}
.date-disabled{opacity:.5;cursor:not-allowed}
.date-checkbox-wrapper mat-checkbox{display:flex;width:100%;padding:10px}
.date-checkbox-content{display:flex;flex-direction:column;align-items:center;width:100%}
.date-day{font-size:18px;font-weight:700;color:var(--color-dark1)}
.date-month-year{font-size:12px;color:var(--color-dark3);margin-bottom:4px}
.fixed-dates-section{background:#f8f9fa;border-radius:12px;padding:16px;margin-bottom:24px}
.fixed-dates-section .dates-header{display:flex;justify-content:space-between;align-items:center;cursor:pointer;padding:8px 12px;border-radius:8px;margin-bottom:12px;background:#fff;box-shadow:0 2px 4px rgba(0,0,0,.05);transition:all .2s}
.fixed-dates-section .dates-header:hover{background-color:#f0f0f0;transform:translateY(-1px);box-shadow:0 4px 8px rgba(0,0,0,.1)}
.fixed-dates-section .dates-header .dates-section-title{margin:0;font-size:16px;font-weight:600;color:var(--color-dark1)}
.fixed-dates-section .dates-header .collapse-icon{transition:transform .3s;color:#666;font-size:28px;min-width:28px;height:28px;display:flex;align-items:center;justify-content:center;user-select:none;cursor:pointer}
.fixed-dates-section .dates-header .collapse-icon.expanded{transform:rotate(180deg)}
.fixed-dates-section .dates-header .collapse-icon:hover{color:#333;background-color:rgba(0,0,0,.1);border-radius:50%}
.fixed-dates-section .dates-content{transition:all .3s;overflow:hidden}
.fixed-dates-section .dates-content.collapsed{max-height:0;opacity:0;margin-top:0;padding-top:0}
.fixed-dates-section .dates-content:not(.collapsed){max-height:1000px;opacity:1}